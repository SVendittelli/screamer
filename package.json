    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "cookie": "0.6.0",
    "madr": "3.0.0",
    "next": "14.1.1",
    "next-swagger-doc": "0.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.3.0",
    "supertokens-auth-react": "0.36.1",
    "supertokens-node": "16.7.1",
    "supertokens-web-js": "0.8.0",
    "swagger-ui-react": "5.11.0",
    "tailwind-merge": "2.2.0",
    "usehooks-ts": "2.9.2",
    "validator": "13.11.0",
    "zod": "3.22.4",
