{
  "name": "screamer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true npm run build",
    "build": "next build",
    "dev": "next dev",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "markdownlint-cli2 --fix **/*.md && prettier --write .",
    "preinstall": "npx npm-only-allow@latest --PM npm",
    "postinstall": "prisma generate",
    "lint": "run-s lint:*",
    "lint:md": "markdownlint-cli2 **/*.md",
    "lint:next": "next lint",
    "lint:all": "prettier --check --ignore-unknown .",
    "migrate": "prisma migrate deploy",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "@prisma/client": "5.7.0",
    "@t3-oss/env-nextjs": "0.7.1",
    "@types/swagger-ui-react": "4.18.3",
    "@vercel/analytics": "1.1.1",
    "@vercel/edge-config": "0.4.1",
    "clsx": "2.0.0",
    "cookie": "0.6.0",
    "madr": "3.0.0",
    "next": "14.0.4",
    "next-swagger-doc": "0.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.12.0",
    "supertokens-auth-react": "0.36.0",
    "supertokens-node": "16.6.4",
    "supertokens-web-js": "0.8.0",
    "swagger-ui-react": "5.10.3",
    "validator": "13.11.0",
    "zod": "3.22.4",
    "zod-validation-error": "2.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.0.4",
    "@playwright/test": "1.40.1",
    "@tailwindcss/typography": "0.5.10",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/cookie": "0.6.0",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@types/validator": "13.11.7",
    "autoprefixer": "10.4.16",
    "cross-env": "7.0.3",
    "dotenv": "16.3.1",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-testing-library": "6.2.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.0",
    "markdownlint": "0.32.1",
    "markdownlint-cli2": "0.11.0",
    "markdownlint-rule-search-replace": "1.2.0",
    "npm-only-allow": "1.2.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "prettier-plugin-packagejson": "2.4.7",
    "prettier-plugin-prisma": "5.0.0",
    "prisma": "5.7.0",
    "tailwindcss": "3.3.6",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": "20.x"
  },
