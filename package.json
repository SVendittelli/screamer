{
  "name": "screamer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true npm run build",
    "build": "next build",
    "build-storybook": "storybook build",
    "commit": "git-cz",
    "dev": "next dev",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "markdownlint-cli2 --fix **/*.md && prettier --write . && next lint --fix",
    "preinstall": "npx npm-only-allow@latest --PM npm",
    "postinstall": "prisma generate",
    "lint": "run-s lint:*",
    "lint:all": "prettier --check --ignore-unknown .",
    "lint:md": "markdownlint-cli2 **/*.md",
    "lint:next": "next lint",
    "migrate": "prisma migrate deploy",
    "prepare": "husky install",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest --watch",
    "test-storybook": "test-storybook --coverage",
    "test:ci": "jest --ci",
    "pretest:storybook": "npm run build-storybook",
    "test:storybook": "start-server-and-test 'http-server storybook-static --port 6006 --silent' http://localhost:6006 test-storybook",
    "posttest:storybook": "nyc report --reporter=lcov -t coverage/storybook --report-dir coverage/storybook"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@prisma/client": "5.8.0",
    "@radix-ui/react-accessible-icon": "1.0.3",
    "@radix-ui/react-avatar": "1.0.4",
    "@t3-oss/env-nextjs": "0.7.1",
    "@types/swagger-ui-react": "4.18.3",
    "@vercel/analytics": "1.1.1",
