{
  "name": "screamer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true npm run build",
    "build": "next build",
    "build-storybook": "storybook build",
    "commit": "git-cz",
    "dev": "next dev",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "markdownlint-cli2 --fix **/*.md && prettier --write . && next lint --fix",
    "preinstall": "npx npm-only-allow@latest --PM npm",
    "postinstall": "prisma generate",
    "lint": "run-s lint:*",
    "lint:all": "prettier --check --ignore-unknown .",
    "lint:md": "markdownlint-cli2 **/*.md",
    "lint:next": "next lint",
    "migrate": "prisma migrate deploy",
    "prepare": "husky",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest --watch",
    "test-storybook": "test-storybook --coverage",
    "test:ci": "jest --ci",
    "pretest:storybook": "npm run build-storybook",
    "test:storybook": "start-server-and-test 'http-server storybook-static --port 6006 --silent' http://localhost:6006 test-storybook",
    "posttest:storybook": "nyc report --reporter=lcov -t coverage/storybook --report-dir coverage/storybook"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@prisma/client": "5.20.0",
    "@radix-ui/react-accessible-icon": "1.1.0",
    "@radix-ui/react-avatar": "1.1.0",
    "@t3-oss/env-nextjs": "0.11.1",
    "@types/swagger-ui-react": "4.18.3",
    "@vercel/analytics": "1.3.1",
    "@vercel/edge-config": "1.3.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "cookie": "0.7.0",
    "madr": "4.0.0",
    "next": "14.2.13",
    "next-swagger-doc": "0.4.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "supertokens-auth-react": "0.47.1",
    "supertokens-node": "20.1.2",
    "supertokens-web-js": "0.13.0",
    "swagger-ui-react": "5.17.14",
    "tailwind-merge": "2.5.2",
    "usehooks-ts": "3.1.0",
    "validator": "13.12.0",
    "zod": "3.23.8",
    "zod-validation-error": "3.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "2.0.2",
