    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.0.2",
    "npm-only-allow": "1.2.6",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-packagejson": "2.4.6",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2"
