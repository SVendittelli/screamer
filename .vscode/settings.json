  "cSpell.words": [
    "automerge",
    "BAFTA",
    "datasources",
    "dockerized",
    "imdb",
    "Metacritic",
    "Metascore",
    "MPAA",
    "omdb",
    "openapi",
    "Passwordless",
    "supertokens",
    "userroles"
  ],
  "conventionalCommits.scopes": [
    "adr",
    "analytics",
    "api",
    "auth",
    "db",
    "demo",
    "deps",
    "git",
    "home",
    "maintenance",
    "next.js",
    "security",
    "users",
    "vercel"
